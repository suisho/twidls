{
  "name": "sandbox",
  "version": "0.0.0",
  "homepage": "https://github.com/suisho/twidls",
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "backbone": "~1.1.0",
    "jquery": "~2.0.3",
    "marionette": "~1.4.1",
    "underscore": "~1.5.2",
    "backbone.stickit": "~0.7.0",
    "backbone.wreqr": "~0.2.0"
  },
  "exportsOverride": {
    "marionette": {
      "js": "lib/core/*.js"
    },
    "backbone.wreqr" : {
      "js": "lib/*.js"
    },
    "*": {
      "js": "*.js",
      "css": "*.css"
    }
  }
}
